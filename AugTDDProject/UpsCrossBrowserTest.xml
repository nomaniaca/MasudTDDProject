<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="UPS CrossBrowserTest Suite" thread-count="2"
	parallel="tests">

	<test name="ChromeBrowser">
		<parameter name="browser" value="ChromeBrowser" />
		<classes>
			<class name="com.ups.testRunner.UpsLoginTestRunner" />
			<class name="com.ups.testRunner.UpsShippingTestRunner" />
			<class name="com.ups.testRunner.UpsTrackingTestRunner" />
		</classes>
	</test> <!-- Test -->

	<test name="FirefoxBrowser">
		<parameter name="browser" value="FirefoxBrowser" />
		<classes>
			<class name="com.ups.testRunner.UpsLoginTestRunner" />
		</classes>
	</test> <!-- Test -->


</suite> <!-- Suite -->
